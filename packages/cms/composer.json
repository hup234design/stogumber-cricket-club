{
    "name": "hup234design/cms",
    "description": "Filament CMS Package",
    "keywords": [
        "hup234design",
        "laravel",
        "cms"
    ],
    "homepage": "https://github.com/hup234design/cms",
    "license": "MIT",
    "authors": [
        {
            "name": "HUP234 Design",
            "email": "dave@hup234design.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "awcodes/filament-table-repeater": "^2.0",
        "awcodes/filament-tiptap-editor": "^3.0",
        "calebporzio/sushi": "^2.4",
        "filament/filament": "^3.0-stable",
        "filament/spatie-laravel-media-library-plugin": "^3.0-stable",
        "illuminate/contracts": "^10.0",
        "jeffgreco13/filament-breezy": "^2.1",
        "pboivin/filament-peek": "^2",
        "ralphjsmit/laravel-filament-seo": "^1.2",
        "ryangjchandler/filament-navigation": "^1.0.0-beta4",
        "spatie/eloquent-sortable": "^4.0",
        "spatie/laravel-honeypot": "^4.3",
        "spatie/laravel-package-tools": "^1.15.0",
        "spatie/laravel-permission": "^5.11",
        "spatie/valuestore": "^1.3"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Hup234design\\Cms\\": "src/",
            "Hup234design\\Cms\\Database\\Factories\\": "database/factories/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Hup234design\\Cms\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hup234design\\Cms\\CmsServiceProvider"
            ],
            "aliases": {
                "Cms": "Hup234design\\Cms\\Facades\\Cms"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
